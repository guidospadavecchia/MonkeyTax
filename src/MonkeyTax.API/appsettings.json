{
  "General": {
    "Title": "MonkeyTax API",
    "Description": "API REST de Monotributo AFIP (Argentina)",
    "GitHubUrl": "https://github.com/guidospadavecchia/MonkeyTax"
  },
  "Scraper": {
    "MonotributoUrl": "https://www.afip.gob.ar/monotributo/categorias.asp",
    "UserAgentsUrl": "https://www.useragents.me/api",
    "Headers": {
      "Referer": "https://www.google.com.ar/",
      "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/avif,image/webp,image/apng,*/*;q=0.8,application/signed-exchange;v=b3;q=0.7",
      "Accept-Encoding": "gzip, deflate, br",
      "Accept-Language": "es-AR,es-ES;q=0.9,es;q=0.8,en-US;q=0.7,en;q=0.6,es-419;q=0.5",
      "Upgrade-Insecure-Requests": "1"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
